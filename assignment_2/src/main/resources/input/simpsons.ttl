@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix fam: <http://www.ifi.uio.no/IN3060/family#>.
@prefix sim: <http://www.ifi.uio.no/IN3060/simpsons#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

# Family
sim:SimpsonFamily rdf:type fam:Family.

# Members of the Simpson Family
sim:Homer rdf:type foaf:Person;
    foaf:name "Homer Simpson";
    foaf:age "36"^^xsd:int;
    fam:hasFamilyMember sim:SimpsonFamily;
    fam:hasSpouse sim:Marge;
    fam:hasSon sim:Bart;
    fam:hasDaughter sim:Lisa;
    fam:hasDaughter sim:Maggie.

sim:Marge rdf:type foaf:Person;
    foaf:name "Marge Simpson";
    foaf:age "34"^^xsd:int;
    fam:hasFamilyMember sim:SimpsonFamily;
    fam:hasSpouse sim:Homer;
    fam:hasSon sim:Bart;
    fam:hasDaughter sim:Lisa;
    fam:hasDaughter sim:Maggie.

sim:Bart rdf:type foaf:Person;
    foaf:name "Bart Simpson";
    foaf:age "10"^^xsd:int;
    fam:hasFamilyMember sim:SimpsonFamily;
    fam:hasFather sim:Homer;
    fam:hasMother sim:Marge.

sim:Lisa rdf:type foaf:Person;
    foaf:name "Lisa Simpson";
    foaf:age "8"^^xsd:int;
    fam:hasFamilyMember sim:SimpsonFamily;
    fam:hasFather sim:Homer;
    fam:hasMother sim:Marge.

sim:Maggie rdf:type foaf:Person;
    foaf:name "Maggie Simpson";
    foaf:age "1"^^xsd:int;
    fam:hasFamilyMember sim:SimpsonFamily;
    fam:hasFather sim:Homer;
    fam:hasMother sim:Marge.

# Blank nodes para representar avós
_:grandparent1 rdf:type foaf:Person;
    foaf:name "Abraham".

_:grandparent2 rdf:type foaf:Person;
    foaf:name "Mona".

# Relações entre Maggie e seus avós
sim:Maggie fam:hasGrandparent _:grandparent1, _:grandparent2.

# Mona
sim:Mona rdf:type foaf:PersonFemale;
    foaf:name "Mona".

# Relação mãe-filha entre Mona e Marge
sim:Mona fam:hasChild sim:Marge.
sim:Marge fam:hasMother sim:Mona.

# Relação pai-filha entre Abraham e Mona
sim:Abraham fam:hasChild sim:Mona.
sim:Mona fam:hasFather sim:Abraham.

# Patty
sim:Patty rdf:type foaf:Person;
    foaf:name "Patty Bouvier".

# Selma
sim:Selma rdf:type foaf:Person;
    foaf:name "Selma Bouvier".

# Herb
sim:Herb rdf:type foaf:Person;
    foaf:name "Herb Powell".

# Relationships
sim:Lisa fam:hasAunt sim:Patty, sim:Selma;
    fam:hasUncle sim:Herb.

sim:Patty fam:isSisterOf sim:Selma.
sim:Selma fam:isSisterOf sim:Patty.
sim:Selma fam:isSisterOf sim:Marge.
sim:Patty fam:isSisterOf sim:Marge.
sim:Marge fam:isSisterOf sim:Selma.
sim:Marge fam:isSisterOf sim:Patty.

# Abraham
sim:Abraham rdf:type foaf:Person;
    foaf:name "Abraham Simpson".

# Relação pai-filho entre Abraham e Homer
sim:Abraham fam:hasChild sim:Homer.
sim:Homer fam:hasFather sim:Abraham.

# Relationship
sim:Marge fam:hasFatherInLaw sim:Abraham.
