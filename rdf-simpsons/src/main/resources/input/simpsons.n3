@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix fam: <http://www.ifi.uio.no/IN3060/family#> .
@prefix sim: <http://www.ifi.uio.no/IN3060/simpsons#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Family
sim:SimpsonFamily a fam:Family .

# Members of the Simpson Family
sim:Homer a foaf:Person ;
    foaf:name "Homer Simpson" ;
    foaf:age "36"^^xsd:int ;
    fam:hasFamilyMember sim:SimpsonFamily ;
    fam:hasSpouse sim:Marge ;
    fam:hasSon sim:Bart ;
    fam:hasDaughter sim:Lisa ;
    fam:hasDaughter sim:Maggie .

sim:Marge a foaf:Person ;
    foaf:name "Marge Simpson" ;
    foaf:age "34"^^xsd:int ;
    fam:hasFamilyMember sim:SimpsonFamily ;
    fam:hasSpouse sim:Homer ;
    fam:hasSon sim:Bart ;
    fam:hasDaughter sim:Lisa ;
    fam:hasDaughter sim:Maggie .

sim:Bart a foaf:Person ;
    foaf:name "Bart Simpson" ;
    foaf:age "10"^^xsd:int ;
    fam:hasFamilyMember sim:SimpsonFamily ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge .

sim:Lisa a foaf:Person ;
    foaf:name "Lisa Simpson" ;
    foaf:age "8"^^xsd:int ;
    fam:hasFamilyMember sim:SimpsonFamily ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge .

sim:Maggie a foaf:Person ;
    foaf:name "Maggie Simpson" ;
    foaf:age "1"^^xsd:int ;
    fam:hasFamilyMember sim:SimpsonFamily ;
    fam:hasFather sim:Homer ;
    fam:hasMother sim:Marge .

# Blank nodes para representar avós
[ a foaf:Person ;
  foaf:name "Abraham" ] .

[ a foaf:Person ;
  foaf:name "Mona" ] .

# Relações entre Maggie e seus avós
sim:Maggie fam:hasGrandparent [ fam:isGrandparentOf sim:Maggie ] .

# Patty
sim:Patty a foaf:Person ;
    foaf:name "Patty Bouvier" .

# Selma
sim:Selma a foaf:Person ;
    foaf:name "Selma Bouvier" .

# Herb
sim:Herb a foaf:Person ;
    foaf:name "Herb Powell" .

# Relationships
sim:Lisa fam:hasAunt sim:Patty, sim:Selma ;
    fam:hasUncle sim:Herb .

sim:Patty fam:isSisterOf sim:Selma .
sim:Selma fam:isSisterOf sim:Patty .

# Abraham
sim:Abraham a foaf:Person ;
    foaf:name "Abraham Simpson" .

# Relationship
sim:Marge fam:hasFatherInLaw sim:Abraham .
